<p id="notice"><%= notice %></p>

<h1>Listing Incomes</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @incomes.each do |income| %>
      <tr>
        <td><%= income.date %></td>
        <td><%= income.price %></td>
        <td><%= link_to 'Edit', edit_income_category_income_path(@income_category, income) %></td>
        <td><%= link_to 'Destroy', income_category_income_path(@income_category, income), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Income', new_income_category_income_path , class: 'btn btn-default'%>
<br>
<br>
<%= link_to 'Back', income_categories_path , class: 'btn btn-default'%>
